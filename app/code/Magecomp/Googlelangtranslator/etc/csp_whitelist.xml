<?xml version="1.0" encoding="UTF-8"?>
<csp_whitelist xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Csp:etc/csp_whitelist.xsd">
    <policies>
        <policy id="style-src">
            <values>
                <value id="google_fallback5" type="host">www.gstatic.com</value>
            </values>
        </policy>
         <policy id="script-src">
                <values>
                    <value id="google_fallback2" type="host">https://translate.googleapis.com</value>
                    <value id="google_fallback7" type="host">http://translate.google.com</value>
                    <value id="google_fallback6" type="host">www.googletagmanager.com</value>
                    <value id="google_fallback8" type="host">https://translate-pa.googleapis.com</value>
                </values>
        </policy>
        <policy id="img-src">
                <values>
                    <!-- <value id="google_fallback3" type="host">https://www.gstatic.com</value> -->
                    <value id="google_fallback9" type="host">https://www.gstatic.com</value>
                    <value id="google_fallback3" type="host">https://translate.googleapis.com</value>
                    <value id="google_fallback10" type="host">https://fonts.gstatic.com</value>
                    <value id="google_fallback11" type="host">http://translate.google.com</value>
                    <value id="google_fallback7" type="host">*.adobe.com</value>
                </values>
        </policy>
         <policy id="connect-src">
                <values>
                    <value id="google_fallback4" type="host">https://translate.googleapis.com</value>
                </values>
        </policy> 
    </policies>
</csp_whitelist>